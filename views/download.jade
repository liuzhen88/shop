extends layout
block title
	title #{data.title}

block content
	.product-explan-container
		.download-container
			.map-title.pro 产品说明文档
			.map-alias PRODUCT DOCUMENTATION
			.line
				img(src='/images/index_underline2.png')
	.cont-container
		.product-document
			ul
				li.document-list.border-right
					.document-list-title 鸡头孩啊
					.document-list-content 
						- for(var i=0;i<data.list.length;i++)
							p #{data.list[i].name} 
				li.document-list.border-right
					.document-list-title 狗头勇啊
					.document-list-content
						- for(var i=0;i<data.list.length;i++)
							p #{data.list[i].name} 
				li.document-list.border-right
					.document-list-title 傻逼狗勇啊
					.document-list-content
						- for(var i=0;i<data.list.length;i++)
							p #{data.list[i].name} 
				li.document-list
					.document-list-title 废物狗勇啊
					.document-list-content
						- for(var i=0;i<data.list.length;i++)
							p #{data.list[i].name} 
	.null-border-bottom
	.download-containers
		.download-container
			.map-title.dn 产品手册下载
			.map-alias PRODUCT MANUALS
			.line
				img(src='/images/index_underline1.png')
		.downlod-files
			.file-list
				span 狗勇挑粪是如何修炼成的_10M.pdf
				a(href="#") 下载
block sub-js
	script(src='/javascripts/download.js')
